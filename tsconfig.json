{
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "target": "esnext",
    "sourceMap": false,
    "preserveConstEnums": false,
    "esModuleInterop": true
  },
  "include": [
    "./src",
    "./test"
  ],
  "exclude": [
    "./node_modules"
  ]
}
