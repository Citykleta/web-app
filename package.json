{
  "name": "citykleta-web",
  "version": "0.1.0",
  "description": "web app for citykleta platform",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test:build": "rollup -c rollup/test.js",
    "test:watch": "npm run test:build -- -w",
    "test": "cat ./test/dist/debug.js | tape-run",
    "compile": "tsc",
    "compile:watch": "npm run compile -- -w",
    "bundle": "rollup -c rollup/build.js",
    "bundle:watch": "npm run bundle -- -w",
    "css": "postcss ./node_modules/mapbox-gl/dist/mapbox-gl.css ./src/app/*.css --dir ./dist/",
    "css:watch": "npm run css -- -w",
    "html": "cpx ./src/*.html ./dist",
    "html:watch": "npm run html -- -w",
    "build": "npm run compile && npm run bundle && npm run css && npm run html",
    "now-build": "NODE_ENV=production npm run build",
    "dev": "npm-run-all -p *:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Citykleta/web-app.git"
  },
  "keywords": [
    "bicycle",
    "serverless",
    "webapp",
    "citykleta"
  ],
  "author": "Laurent RENARD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Citykleta/web-app/issues"
  },
  "homepage": "https://github.com/Citykleta/web-app#readme",
  "devDependencies": {
    "@types/mapbox-gl": "^0.51.12",
    "cpx": "^1.5.0",
    "koa": "^2.7.0",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.3",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^12.0.1",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "typescript": "^3.5.3",
    "zora": "^3.0.2"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.6.0",
    "@mapbox/polyline": "^1.1.0",
    "@turf/midpoint": "^5.1.5",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "mapbox-gl": "^1.2.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  }
}
