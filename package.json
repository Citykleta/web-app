{
  "name": "citykleta-web",
  "version": "0.1.0",
  "description": "web app for citykleta platform",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc",
    "compile:watch": "npm run compile -- -w",
    "bundle": "rollup -c rollup/build.js",
    "bundle:watch": "npm run bundle -- -w",
    "css": "postcss ./src/theme/main.css -u postcss-import -o ./dist/theme.css",
    "css:watch": "npm run css -- -w",
    "html": "cpx ./src/*.html ./dist",
    "html:watch": "npm run html -- -w",
    "build": "npm run compile && npm run bundle && npm run css && npm run html",
    "now-build": "NODE_ENV=production npm run build",
    "dev": "npm-run-all -p compile:watch bundle:watch css:watch html:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Citykleta/web-app.git"
  },
  "keywords": [
    "bicycle",
    "serverless",
    "webapp",
    "citykleta"
  ],
  "author": "Laurent RENARD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Citykleta/web-app/issues"
  },
  "homepage": "https://github.com/Citykleta/web-app#readme",
  "devDependencies": {
    "@mapbox/mapbox-sdk": "^0.5.0",
    "@mapbox/polyline": "^1.0.0",
    "cpx": "^1.5.0",
    "mapbox-gl": "^0.53.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "rollup": "^1.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "smart-table-events": "^1.0.10",
    "typescript": "^3.3.3333",
    "zora": "^3.0.0-2"
  }
}
