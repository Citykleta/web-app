{
  "name": "citykleta-web",
  "version": "0.1.0",
  "description": "web app for citykleta platform",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test:build": "rollup -c rollup/test.js",
    "test:watch": "npm run test:build -- -w",
    "test": "node ./test/dist/debug.js | tap-mocha-reporter classic",
    "compile": "tsc",
    "compile:watch": "npm run compile -- -w",
    "bundle": "rollup -c rollup/build.js",
    "bundle:watch": "npm run bundle -- -w",
    "css": "postcss ./node_modules/mapbox-gl/dist/mapbox-gl.css ./src/app/elements/*.css --dir ./dist/",
    "css:watch": "npm run css -- -w",
    "html": "cpx ./src/*.html ./dist",
    "html:watch": "npm run html -- -w",
    "build": "npm run compile && npm run bundle && npm run css && npm run html",
    "now-build": "NODE_ENV=production npm run build",
    "dev": "npm-run-all -p *:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Citykleta/web-app.git"
  },
  "keywords": [
    "bicycle",
    "serverless",
    "webapp",
    "citykleta"
  ],
  "author": "Laurent RENARD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Citykleta/web-app/issues"
  },
  "homepage": "https://github.com/Citykleta/web-app#readme",
  "devDependencies": {
    "@mapbox/mapbox-sdk": "^0.5.0",
    "@mapbox/polyline": "^1.0.0",
    "cpx": "^1.5.0",
    "fuse-box": "^3.7.1",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0",
    "mapbox-gl": "^0.53.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "postcss-color-function": "^4.0.1",
    "postcss-import": "^12.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rollup": "^1.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-uglify": "^6.0.2",
    "tap-mocha-reporter": "^3.0.7",
    "typescript": "^3.3.3333",
    "zora": "^3.0.1"
  },
  "dependencies": {
    "evil-icons": "^1.10.1",
    "feather-icons": "^4.19.0",
    "grommet-icons": "^4.2.0",
    "hybrids": "^2.0.0",
    "ionicons": "^4.5.5",
    "octicons": "^8.5.0",
    "redux-subscriber": "^1.1.0"
  }
}
